    <!-- All Jquery -->
    <!-- ============================================================== -->
    {{> plugins}}
    <script src="/js/client/datatable/kesimpulanassessmentprodi.js"></script>
    <script src="/js/client/modal/kesimpulanassessmentprodi.js"></script>
</body>

</html>
